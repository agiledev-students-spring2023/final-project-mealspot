{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "prefer-template": "off",
      "radix": "off",
      "global-require": "off", 
      "no-restricted-globals": "off",
      "prefer-const": "off",
      "no-undef": "off",
      "node/no-unpublished-require": "off"
    }
  }